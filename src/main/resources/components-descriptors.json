  "componentDescriptors": [
    {
      "hidden": false,
      "displayName": "XPath Evaluate",
      "fullyQualifiedName": "com.reedelk.xml.component.XPathEvaluate",
      "componentType": "PROCESSOR",
      "componentPropertyDescriptors": [
        {
          "hintValue": "//book[@year\u003e2001]/title/text()",
          "displayName": "XPath Expression",
          "propertyName": "expression",
          "defaultValue": "",
          "propertyInfo": "Sets the XPath expression to be evaluated. It can be a dynamic expression. \u003cbr\u003eExamples: \u003cul\u003e\u003cli\u003e\u003ci\u003eStatic\u003c/i\u003e: //book[@year\u003e2001]/title/text()\u003c/li\u003e\u003cli\u003e\u003ci\u003eStatic\u003c/i\u003e: count(//book/title)\u003c/li\u003e\u003cli\u003e\u003ci\u003eStatic\u003c/i\u003e: boolean(/inventory/book/price[text() \u003e 14])\u003c/li\u003e\u003cli\u003e\u003ci\u003eStatic\u003c/i\u003e: //ns2:bookStore/ns2:book/ns2:name/text()\u003c/li\u003e\u003cli\u003e\u003ci\u003eDynamic\u003c/i\u003e: \u0027boolean(/inventory/book/price[text() \u003e \u0027 + message.attributes().queryParams.price + \u0027])\u0027\u003c/li\u003e\u003c/ul\u003e",
          "propertyType": {
            "classname": "com.reedelk.module.descriptor.model.TypeDynamicValueDescriptor",
            "instance": {
              "type": "com.reedelk.runtime.api.script.dynamicvalue.DynamicString"
            }
          },
          "whenDescriptors": []
        },
        {
          "displayName": "XPath Context",
          "propertyName": "configuration",
          "defaultValue": "###USE_DEFAULT_VALUE###",
          "propertyInfo": "The context configuration can be used when the XPath expression uses prefixes in the definition. The configuration allows to define the prefixes \u003e namespaces mapping.",
          "propertyType": {
            "classname": "com.reedelk.module.descriptor.model.TypeObjectDescriptor",
            "instance": {
              "shared": "YES",
              "collapsible": "NO",
              "typeFullyQualifiedName": "com.reedelk.xml.component.XPathConfiguration",
              "objectProperties": [
                {
                  "displayName": "Prefix \u003e Namespace mappings",
                  "propertyName": "prefixNamespaceMap",
                  "defaultValue": "###USE_DEFAULT_VALUE###",
                  "propertyType": {
                    "classname": "com.reedelk.module.descriptor.model.TypeMapDescriptor",
                    "instance": {
                      "tabPlacement": "TOP",
                      "type": "java.util.Map"
                    }
                  },
                  "whenDescriptors": []
                }
              ],
              "type": "com.reedelk.module.descriptor.model.TypeObjectDescriptor$TypeObject"
            }
          },
          "whenDescriptors": []
        }
      ]
    },
